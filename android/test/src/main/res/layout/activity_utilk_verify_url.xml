<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".utilk.kotlin.UtilKVerifyUrlActivity">

        <EditText
            android:id="@+id/utilk_verify_url_txt_port"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:hint="UtilKVerifyUrl(校验端口)"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/utilk_verify_url_btn_port"
            style="@style/BasicKStyle_Btn"
            android:layout_marginBottom="10dp"
            android:text="UtilKVerifyUrl(校验端口)"
            tools:ignore="HardcodedText" />
    </LinearLayout>
</layout>